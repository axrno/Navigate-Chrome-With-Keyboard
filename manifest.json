{
    "manifest_version": 2,
    "name": "Keyboard Navigation",
    "version": "1.0",
    "description": "Navigate and scroll pages using the keyboard",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["activeTab", "<all_urls>"]
}
